{
  "Environment": "Docker",
  "Serilog": {
    "Using": [
      "Serilog",
      "Serilog.Exceptions",
      "Serilog.Sinks.Elasticsearch",
      "Serilog.Enrichers.Environment",
      "Serilog.Formatting.Elasticsearch"
    ],
    "MinimumLevel": {
      "Default": "Verbose",
      "Override": {
        "System": "Verbose",
        "Microsoft": "Verbose",
        "Microsoft.EntityFrameworkCore": "Verbose"
      }
    },
    "WriteTo": {
      "1": {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://nosqldb:9200",
          "indexFormat": "schoolmngr-services-{0:yyyy-MM}",
          "templateName": "schoolmngrTemplate",
          "AutoRegisterTemplate": true,
          "AutoRegisterTemplateVersion": "ESv7"
        }
      }
    },
    "Enrich": [ "WithExceptionDetails", "DebuggerAttached" ]
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": false,
    "EnvironmentInfoEndpointEnabled": true
  },
  "InfraSection": {
    "EventBus": {
      "UseEventBus": true,
      "Host": "msbroker",
      "Username": "guest",
      "Password": "guest",
      "QuequeName": "notification_queque"
    }
  }
}